{
  "name": "webforge/utils",
  "type": "library",
  "description": "A basic lib for file system and utils",
  "keywords": ["framework", "util", "system"],
  "homepage": "http://github.com/webforge-labs/webforge-utils",
  "license": "MIT",
  "authors": [
    {"name": "Philipp Scheit", "email": "p.scheit@ps-webforge.com"}
  ],
  "require": {
    "php": ">=8.0.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.6@stable",
    "mockery/mockery": "@stable",
    "webforge/collections": "^1.0@dev",
    "friendsofphp/php-cs-fixer": "^3",
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-0": {"Webforge\\Common": ["src/php/", "tests/php/"]}
  },
  "minimum-stability": "stable",
  "config": {
    "platform": {
      "php": "8.4"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
